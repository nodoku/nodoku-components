{
    "name": "nodoku-components",
    "version": "{{version}}",
    "license": "{{license}}",
    "exports": {
        ".": {
            "types": "./types/index.d.ts",
            "import": "./esm/index.js"
        },
        "./tailwind": {
            "types": "./types/tailwind/index.d.ts",
            "require": "./esm/tailwind/index.js"
        },

    {{#hljsThemes}}
        "./styles/hljs/{{.}}": {
            "import": "./esm/hljs/styles/prefixed-{{.}}.css"
        },
    {{/hljsThemes}}

        "./package.json": "./package.json"
    },
    "module": "esm/index.js",
    "types": "types/index.d.ts",
    "type": "module",
    "peerDependencies": {
    {{#peerDeps}}
        "{{{name}}}": "{{{ver}}}"{{^isLast}},{{/isLast}}
    {{/peerDeps}}
    },
    "dependencies": {
    {{#deps}}
        "{{name}}": "{{ver}}"{{^isLast}},{{/isLast}}
    {{/deps}}
    }
}
